{
    "manifest_version": 3,
    "name": "Tweaklify",
    "description": "Tweaklify your web experience. No code, just clicks.",
    "version": "1.0.1",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "icons": {
        "32": "icon.png"
    }
}
